#!/bin/bash

# Declare an array to hold file paths or URLs
declare -a inputs=()

# Function to process each input (file path or URL)
function process_input {
    local input=$1
    echo "Processing $input..."
    # Your processing logic here
}

# Main script logic: iterate over the array and process each element
for input in "${inputs[@]}"; do 
    process_input "$input"
done
